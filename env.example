# Google Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
NEXT_PUBLIC_GA_ADS_ID=AW-XXXXXXXXXX

# Google Search Console
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=your-google-verification-code

# RealScout API (if using)
REALSCOUT_API_KEY=your-realscout-api-key
REALSCOUT_API_URL=https://api.realscout.com/v3

# Contact Form (if using external service)
CONTACT_FORM_ENDPOINT=https://your-form-service.com/submit
CONTACT_FORM_API_KEY=your-form-api-key

# Email Service (if using)
EMAIL_SERVICE_API_KEY=your-email-service-key
EMAIL_SERVICE_ENDPOINT=https://your-email-service.com/send

# Database (if using)
DATABASE_URL=your-database-connection-string

# Authentication (if using)
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=https://lasvegasrelocationservices.com

# Performance Monitoring
NEXT_PUBLIC_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_ERROR_TRACKING=true

# Feature Flags
NEXT_PUBLIC_ENABLE_BLOG=true
NEXT_PUBLIC_ENABLE_RESOURCES=true
NEXT_PUBLIC_ENABLE_CALCULATOR=true
